{
  "name": "coolbox",
  "version": "1.1.7",
  "displayName": "Coolbox",
  "author": "TorNato & SleepyStew",
  "description": "An extension to extend and customise Schoolbox.",
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch --mode development --minify false",
    "dev": "vite",
    "serve:firefox": "web-ext run --start-url \"about:debugging#/runtime/this-firefox\"  --source-dir ./dist/",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://schoolbox.donvale.vic.edu.au\" --source-dir ./dist/"
  },
  "license": "MIT",
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "^5.0.0",
    "@types/chrome": "^0.0.228",
    "@types/webextension-polyfill": "^0.10.0",
    "@vitejs/plugin-vue": "^4.2.1",
    "autoprefixer": "^10.4.14",
    "daisyui": "^4.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "vite": "~4.3.3",
    "web-ext": "^7.6.2"
  },
  "dependencies": {
    "@vuepic/vue-datepicker": "^5.4.0",
    "@vueuse/components": "^10.2.1",
    "@vueuse/core": "^10.2.1",
    "dompurify": "^3.0.8",
    "vue": "^3.2.47",
    "vue-shadow-dom": "^4.2.0",
    "vue3-smooth-dnd": "^0.0.5",
    "webextension-polyfill": "^0.10.0"
  }
}
